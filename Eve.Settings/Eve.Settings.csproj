<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.1;net6.0</TargetFrameworks>
    <PackageId>Eve.Settings</PackageId>
    <Version>0.1.0</Version>
    <Authors>hoseinyeganloo@gmail.com</Authors>
    <Company>Eve</Company>
    <PackageDescription>Just enough settings manager.</PackageDescription>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <Copyright>Hosein Yeganloo - 2020</Copyright>
    <PackageTags>Settings Manager;Simple;Json Handling</PackageTags>
    <RepositoryURL>https://github.com/Yeganloo/Eve.Settings</RepositoryURL>
  </PropertyGroup>
  
  <ItemGroup>
    <None Include="../LICENSE.txt" Pack="true" PackagePath="$(PackageLicenseFile)"/>
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="Eve.Caching" Version="0.1.4" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

</Project>
